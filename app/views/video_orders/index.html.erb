<p id="notice"><%= notice %></p>

<h1>Video Orders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @video_orders.each do |video_order| %>
      <tr>
        <td><%= link_to 'Show', video_order %></td>
        <td><%= link_to 'Edit', edit_video_order_path(video_order) %></td>
        <td><%= link_to 'Destroy', video_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video Order', new_video_order_path %>
