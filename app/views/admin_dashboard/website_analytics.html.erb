
<div class="col-md-12">
    <div class="card">
        <div class="card-min-height-50vh">
            <div class="card-header ">
                <h4 class="card-title">Users</h4>
                <p class="card-category">All Users</p>
            </div>
            <div class="card-body table-full-width table-responsive">
              <div class="padding_5">

                <table class="table">
                  <thead>
                    <tr>
                      <th class="center">Referring</th>
                      <th class="center">Count</th>


                    </tr>
                  </thead>
                  <tbody>
                    <% @visits_referring.each do |referring_domain, count| %>
                    <tbody>
                        <tr>
                          <td class="center"><%= referring_domain %></td>
                          <td class="center"><%= count %></td>
                        </tr>
                  <% end %>
                </table>


              </div>
            </div>
        </div>
    </div>
</div>
