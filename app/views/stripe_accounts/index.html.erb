<p id="notice"><%= notice %></p>

<h1>Stripe Accounts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stripe_accounts.each do |stripe_account| %>
      <tr>
        <td><%= link_to 'Edit', edit_user_stripe_account_path(stripe_account) %></td>
        <td><%= link_to 'Destroy', user_stripe_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stripe Account', new_user_stripe_account_path %>
